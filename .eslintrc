{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "error", {
        "semi": true
      }
    ],
    "react/destructuring-assignment": "off",
    "no-param-reassign": "off",
    "no-nested-ternary": "off",
    "no-prototype-builtins": "off"
  }
}